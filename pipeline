pipeline {

    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building...'
                sh 'npm install'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing...'
                sh 'npm test'
            }
        stage('Build Fail/Pass') {
            steps {
                echo 'Build Fail/Pass...'
                sh 'npm Build Fail Condition'
            }
            stage('Exporting results') {
                 steps {
                    echo 'Exporting Results to HP SSC...'
                    sh 'npm results export condition'
                }
        }
    }
}
